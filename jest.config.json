{
    "collectCoverage": true,
    "testRegex": "((\\.|/*.)(spec|test))\\.(js|jsx|tsx)?$",
    "reporters": [ "default" ],
    "testEnvironment" : "jsdom",
    "modulePaths": [
        "<rootDir>/src/"
      ],
    "transform": {
      "^.+\\.(ts|js|tsx|jsx)$": "@swc/jest"
    },
    "transformIgnorePatterns" : [
      "node_modules/(?!(react-table-provider))"
    ],
    "moduleNameMapper": {
        "react-table-odata-source": "<rootDir>/src/index.ts"
      }
}